/*
n = 1745542018215^a * 2017^d * 408^k
  = 2^(3*k) * (3^(2*a+k) * 79^a * 859^a * 1123^a) * (5^a * 17^k * 509^a * 2017^d)
=>
(1) a is odd or k is odd, F(n) = 0
(2) otherwise, F(n) = 4*(a+1)*(k+1)*(a+1)*(d+1)
*/


{
s = 0;
for (a = 1, 100, for (d = 1, 100, for (k = 1, 100,
    if (a % 2 == 1 || k % 2 == 1,
        s += Mod(1, 1000000007),
        s += Mod(a*d*k, 1000000007)^(4*(a+1)*(k+1)*(a+1)*(d+1));
    );
)));
ans = lift(s);
print(ans);
}
