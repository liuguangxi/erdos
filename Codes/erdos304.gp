/*
(sqrt(3) - sqrt(2))^n = sqrt(a(n)) - sqrt(a(n) - 1)
a(n) = 3, 25, 243, 2401,...
a(n) = 11*a(n-1) - 11*a(n-2) + a(n-3) for n >= 4
*/


{
N = 999999999999999999;
M = 1000000007;
A = [11, -11, 1; 1, 0, 0; 0, 1, 0];
x0 = [243, 25, 3]~;
xn = Mod(A, M)^(N - 3) * Mod(x0, M);
res = lift(xn[1]);
print(res);
}
