/*
C: x^2 + y^2 = 2003^2, x > 0, y > 0
L1: y = x + 1
L2: y = x - 1
*/


{
x1 = 0;
y1 = 1;
x2 = 8024017^(1/2)/2 - 1/2;
y2 = 8024017^(1/2)/2 + 1/2;
t = acos((x1*x2+y1*y2)/(sqrt(x1^2+y1^2)*sqrt(x2^2+y2^2)));
s1 = 1/2*2003^2*t - 1/2*1*2003*sin(t);
prob = s1*2/(1/4*Pi*2003^2);
ans = floor(prob*10^8);
print(ans);
}
